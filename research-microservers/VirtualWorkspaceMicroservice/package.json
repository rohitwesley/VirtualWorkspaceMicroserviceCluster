{
  "name": "virtualworkspace-microserver",
  "version": "0.0.1",
  "private": true,
  "description": "Virtual Workspace Microservice",
  "main": "/dist/index.js",
  "watch": {
    "build": {
      "patterns": [
        "public",
        "src"
      ],
      "extensions": "ts"
    },
    "start": "dist/index.js"
  },
  "scripts": {
    "build": "webpack",
    "build:local": "NODE_ENV=development npm run build",
    "watch": "npm-watch",
    "start": "node -r dotenv/config dist/index.js",
    "start:local": "npm run start | pino-pretty -c -l -t SYS:standard",
    "lint": "eslint --ext js,ts src test",
    "lint:fix": "npm run lint -- --fix",
    "lint:ci": "eslint --ext js,ts src test -f junit -o lint-report.xml",
    "test": "echo TODO",
    "test:ci": "echo TODO",
    "clean": "rm -rf dist node_modules",
    "docker:build": "docker build -t virtualworkspace-microservice .",
    "docker:run": "docker run -it --rm -p 8000:8000 --env-file ./.env virtualworkspace-microservice"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wesley/virtualworkspace-microservice.git"
  },
  "keywords": [
    "microservice,jwt,jsonwebtoken"
  ],
  "author": "Wesley T <wesleythomas360@gmail.com> (http://tecrt.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wesley/virtualworkspace-microservice/issues"
  },
  "homepage": "https://github.com/wesley/virtualworkspace-microservice#readme",
  "directories": {
    "test": "test"
  },
